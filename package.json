  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "fmt": "prettier --write \"src/**/*.ts\"",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
